{
  "name": "altv-launcher",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "tauri": "tauri"
  },
  "dependencies": {
    "@tauri-apps/api": ">=2.0.0-beta.0",
    "@tauri-apps/plugin-shell": "^2.0.0-beta.6"
  },
  "devDependencies": {
    "@tauri-apps/cli": ">=2.0.0-beta.0",
    "prettier": "^3.3.1",
    "typescript": "^5.0.2",
    "vite": "^5.0.0"
  },
  "prettier": {
    "tabWidth": 2,
    "printWidth": 80,
    "singleQuote": true,
    "semi": true,
    "arrowParens": "always",
    "trailingComma": "all",
    "overrides": [
      {
        "files": [
          "**/*.css",
          "**/*.scss",
          "**/*.sass",
          "**/*.html"
        ],
        "options": {
          "singleQuote": false
        }
      }
    ]
  },
  "engines": {
    "node": ">=20.9.0"
  }
}
